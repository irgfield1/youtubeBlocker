{
    "manifest_version": 2,
    "name": "Youtube Tracker",
    "description": "Creates a youtube history with URLS, video titles, and channels",
    "version": "0.1.0",
    "homepage_url": "https://github.com/irgfield1",
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "proxy",
        "<all_urls>"
    ],
    "content_security_policy": "script-src https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js 'self'; object-src 'self'",
    "background": {
        "scripts": [
            "node_modules/webextension-polyfill/dist/browser-polyfill.js",
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "youtube-tracker options",
        "default_popup": "options.html"
    },
    "icons": {
        "48": "assets/icons/chillout-48.png"
    }
}