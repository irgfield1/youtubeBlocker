{
    "manifest_version": 2,
    "name": "Youtube Tracker",
    "description": "Creates a youtube history with URLS, video titles, and channels",
    "version": "0.1.0",
    "homepage_url": "https://github.com/irgfield1",
    "permissions": [
        "storage",
        "tabs",
        "alarms",
        "proxy",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "browser_action": {
        "browser_style": true,
        "default_title": "youtube-tracker options",
        "default_popup": "options.html"
    },
    "icons": {
        "48": "icons/chillout-48.png"
    }
}